{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "outputDirectory": "dist",
  "rewrites": [{ "source": "/_api/(.*)", "destination": "/api/$1" }],
  "headers": [
    { "source": "/_api/(.*)", "headers": [
      { "key": "Cache-Control", "value": "no-store" }
    ]},
    { "source": "/", "headers": [
      { "key": "Cache-Control", "value": "no-store" }
    ]},
    { "source": "/index.html", "headers": [
      { "key": "Cache-Control", "value": "no-store" }
    ]}
  ]
}
